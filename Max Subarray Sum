#include <iostream>
#include<vector>
#include<climits>

using namespace std;

int main()
{int n=7;
int arr[7]={3,-4,5,4,-1,7,-8};
int maxsum=INT_MIN;
for(int st=0;st<n;st++){
    int cursum=0;
    for(int end=st;end<n;end++ ){
        cursum+=arr[end];
        maxsum=max(cursum ,maxsum);
    }
}  cout<<" max subarray sum="<<maxsum<<endl;
    return 0;
}
