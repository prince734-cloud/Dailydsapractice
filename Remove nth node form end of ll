ListNode* removeNthFromEnd(ListNode* head, int n) {

        ListNode* fast = head;
        ListNode* slow =head;

        for(int i=0;i<n;++i){
            fast =fast->next;
        }

        if(fast==NULL){
            ListNode* newHead = head->next;
            delete head;
            return newHead;
        }

        while(fast->next != NULL){
            slow=slow->next;
            fast = fast->next;

        }

        ListNode* remove = slow->next;
        slow->next = slow->next->next;
        delete remove;

          return head;
    }
  
