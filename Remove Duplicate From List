ListNode* deleteDuplicates(ListNode* head) {   
        ListNode* temp=head;
        while(temp!= NULL && temp->next!=NULL){

            if(temp->val==temp->next->val){

                ListNode* remove =temp->next;
                temp->next=temp->next->next;

                delete remove;
                
            }
            else{
                temp=temp->next;
            }
         
        }
        return head;

        
    }
