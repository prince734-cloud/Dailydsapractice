int maxArea(vector<int>& height) {
       /* int width ;
         int len ;
         int area; */
         int maxarea =0;
         int i =0;
         int j=height.size()-1;
           while(i<j){
            
           int width = j-i;
           int len = min(height[i] ,height[j]);
           
          int area= len*width;
           maxarea = max(maxarea , area);
           height[i]<height[j] ? i++ : j--;

          
        
           
        }
        return maxarea;

    }
