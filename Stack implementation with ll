#include <iostream>
#include<list>
using namespace std;
 class stack{
     list<int> ls;
     public:
     void push(int val){ //push_front
         ls.push_front(val);
     }
     void pop(){
         ls.pop_front();
     }
     
     int top(){
         return ls.front();//head of ll
     }
     
     bool isempty(){
         return ls.size()==0 ;
     }
 };

int main()
{ stack s;
   s.push(4);
   s.push(3);
   s.push(2);
   s.push(1);
   
   while(!s.isempty()){
       cout<<s.top()<<endl;
       s.pop();
       
   }
   cout<<endl;
  
    
    return 0;
}
