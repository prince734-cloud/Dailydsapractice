#include <iostream>
#include<vector>
using namespace std;
int subarraysum(vector<int>& nums ,int k){
    int n=nums.size();
    int count =0;
    
    for(int i=0;i<n;i++){
        int sum=0;
        for(int j=i;j<n;j++){
            sum+=nums[j];
            if(sum==k) count++;
        }
    }
    return count;
}

int main(){
    
vector<int> nums={9,4,20,3,10,5};
int k=33;
cout<<subarraysum(nums,k)<<endl;
 
    

    return 0;
}
