#include <iostream>
#include <vector>
#include <stack>
using namespace std;
int findCelebrity(vector<vector<int>> arr){ //O(n)
    stack<int> s;
    int n =arr.size();
    for(int i=0;i<n;i++){
        s.push(i);
    }
    while(s.size()>1){
        int j =s.top();
        s.pop();
        int k =s.top();
        s.pop();
        if(arr[j][k]==0){
            s.push(j);
        }
        else{
            s.push(k);
        }
    }
    int celeb = s.top();
    for(int i=0;i<n;i++){
        if((i!=celeb) && (arr[i][celeb]==0 || arr[celeb][i]==1)){
            return -1;
        }
    }
    return celeb;
    
}

int main()
{
          vector<vector<int>> arr ={ {0,0,1} ,{0,0,1} ,{0,0,0} };
            int ans= findCelebrity(arr);
            cout<< " celebrity is "<< ans<<endl;
            
    
    return 0;
}
