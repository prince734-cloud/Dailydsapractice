
#include <iostream>
#include <climits>
using namespace std;
int getmaxsum(int matx[][3] ,int rows ,int cols){
    int maxrowsum = INT_MIN;
    for(int i=0;i<rows;i++){
        int rowsum =0;
        for(int j=0;j<cols;j++){
            rowsum+=matx[i][j];
            
        }
        
        maxrowsum=max(maxrowsum,rowsum);
} 
  return maxrowsum;
}

int main()
{
    int matrix[3][3]={{1,2,3},{4,25,6},{7,8,9}};
    int row=3;
    int cols=3;
    cout<<getmaxsum(matrix , row ,cols)<<endl;
    return 0;
}
