vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int> st(nums1.begin(), nums1.end()); // store nums1
        vector<int> result;

        for (int x : nums2) {
            // check using hash set find()
            if (st.find(x) != st.end()) {
                result.push_back(x); // common element
                st.erase(x);         // remove to keep result unique
            }
        }
        return result;
    }
