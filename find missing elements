 vector<int> findMissingElements(vector<int>& nums) {
        int mn = *min_element(nums.begin(), nums.end());
        int mx = *max_element(nums.begin(), nums.end());
        unordered_set<int> seen(nums.begin(), nums.end());
        
        vector<int> missing;
        for (int x = mn + 1; x < mx; ++x) {
            if (!seen.count(x)) {
                missing.push_back(x);
            }
        }
        return missing;
    }
